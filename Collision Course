t=int(input())
D={}
Collision=0
for i in range(t):
    x,y,speed=map(int,input().split())
    sq_time=(x**2+y**2)/(speed**2)
    if (D.get(sq_time) == None):
        D[sq_time]=1
    else:
        D[sq_time]+=1
for keys in D:
    if (D[keys] != 1):
        Collision=Collision+(D[keys]*(D[keys]-1))//2
print(Collision)
    
